#include <iostream>
#include <string>
using namespace std;

int main() {
	// Complete the program
     string a,b;
     char A, B;
     cin >> a >> b;
     
     cout << a.length() << " " << b.length() << endl;
     
    
     cout << a + b << endl;
     
     
     A = a[0];
     B = b[0];
     a[0] = B;
     b[0] = A;
     cout << a << " " << b;
     
     
    return 0;
}
